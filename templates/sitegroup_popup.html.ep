% title 'Sitegroup popup';
% layout 'popup';
% my $sitegroup = $c->stash('sitegroup');
<A href="/sitegroup/details?id=<%= $sitegroup->{'id'} %>"><%= $sitegroup->{'name'} %></A><BR>
<HR>
<A href="javascript:begin_drag_object('sitegroup<%= $sitegroup->{'id'} %>')">Move...</A><BR>
<HR>
% foreach my $site (@{$c->stash('sites')}) {
<A href="/site/map?id=<%= $site->{'id'} %>"><%= $site->{'name'} %></A><BR>
% }


