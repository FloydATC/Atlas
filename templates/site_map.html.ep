% title 'Site ';
% layout 'plain';
% my $site = $c->stash('site');
% my $width = $c->stash('width');
% my $height = $c->stash('height');
%= include 'menu'
This is site <%= $site->{'id'} %> "<%= $site->{'name'} %>"<BR>
<object data="/site/svg?id=<%= $site->{'id'} %>&width=<%= $width %>&height=<%= $height %>" 
